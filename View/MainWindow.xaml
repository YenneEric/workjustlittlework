<Window x:Class="View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200">
    <Grid>

        <local:HomePage x:Name="HomePage" Visibility="Visible" />
        <local:InsertData x:Name="InsertData" Visibility="Hidden" />
        <local:AddPlayerPage x:Name="AddPlayerPage" Visibility="Hidden" />
        <local:AddGame x:Name="AddGame" Visibility="Hidden" />

        <local:EditPlayer x:Name="EditPlayer" Visibility="Hidden" />
        <local:EditGame x:Name="EditGame" Visibility="Hidden" />
        <local:ViewStats x:Name="ViewStats" Visibility="Hidden" />
        <local:EditStats x:Name="EditStats" Visibility="Hidden" />




        <local:MostTouchdowns x:Name="MostTouchdowns" Visibility="Hidden" />
        <local:ConfrenceTeamRank x:Name="ConfrenceTeamRank" Visibility="Hidden" />
        <local:MostTeamYards x:Name="MostTeamYards" Visibility="Hidden" />
        <local:TopScoring x:Name="TopScoring" Visibility="Hidden" />
        <local:AddSeasonTeamOrConference x:Name="AddSeasonTeamOrConference" Visibility="Hidden" />

    </Grid>
</Window>